{
  "builds": [
    {
      "src": "python_chat/wsgi.py",
      "use": "@vercel/python"
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "python_chat/wsgi.py"
    }
  ]
}
